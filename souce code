#include<iostream>
using namespace std;
#define max 100

struct stack{
	char top=-1;
	int data[max];
};
void push(int x){
data[top++]=x;
}

void pop(){
	if(top==-1){
		cout<<"Tidak dapat ditambah";
	}
	else{
		top--;
	}
}

int atas(){
	return data[top];
}

int kosong(){
	if(top==-1){
	return 1;}
	else{
	return 0;}	
}

int main(){

}
