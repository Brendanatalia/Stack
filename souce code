#include<iostream>
using namespace std;

struct stack{
	char top=-1;
	 char data[100];
	
	void push(int x){
		data[top++]=x;
}

int pop(){
	if(top==-1){
		cout<<"Tidak dapat dipop";
		return top;
	}
	else{
		top--;
	}
}

int push(char x){
	if(top==100-1){
		cout<<"Stack penuh";
		return top;
	}
	data[++top]=x;
}

int atas(){
	return data[top];
}

int kosong(){
	if(top==-1){
	return 1;}
	else{
	return 0;}	
}

int main(){

}
